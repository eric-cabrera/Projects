{
    "name": "agentportalapp",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "dev": "vite",
        "build": "run-p type-check build-only",
        "preview": "vite preview",
        "test": "vitest run --environment jsdom --root src/",
        "build-only": "vite build",
        "type-check": "vue-tsc --noEmit",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore --max-warnings=0",
        "lint:fix": "npm run lint -- --fix",
        "prettier": "prettier src src/tests --check",
        "prettier:fix": "npm run prettier -- --write",
        "format": "npm run prettier:fix && npm run lint:fix"
    },
    "dependencies": {
        "@assurity/newassurelink-client": "^0.0.20250415-2",
        "@gtm-support/vue-gtm": "^3.1.0",
        "@newrelic/browser-agent": "^1.264.0",
        "@tanstack/vue-query": "^5.51.24",
        "axios": "^1.7.4",
        "dayjs": "^1.11.13",
        "lodash": "^4.17.21",
        "pinia": "^2.2.2",
        "vee-validate": "^4.13.2",
        "vue": "^3.5.4",
        "vue-router": "^4.4.3",
        "vuetify": "^3.7.0"
    },
    "devDependencies": {
        "@babel/types": "^7.25.2",
        "@mdi/font": "7.4.47",
        "@pinia/testing": "^0.1.5",
        "@rushstack/eslint-patch": "^1.10.4",
        "@tsconfig/node20": "^20.1.4",
        "@types/file-saver": "^2.0.7",
        "@types/jsdom": "^21.1.7",
        "@types/lodash": "^4.17.7",
        "@types/node": "^22.4.1",
        "@vitejs/plugin-vue": "^5.1.2",
        "@vue/eslint-config-prettier": "^9.0.0",
        "@vue/eslint-config-typescript": "^13.0.0",
        "@vue/test-utils": "^2.4.6",
        "@vue/tsconfig": "^0.5.1",
        "eslint": "^8.57.0",
        "eslint-plugin-prettier": "^5.2.1",
        "eslint-plugin-vue": "^9.27.0",
        "jsdom": "^24.1.1",
        "npm-run-all": "^4.1.5",
        "postcss-nested": "^6.2.0",
        "postcss-preset-env": "^10.0.2",
        "sass": "^1.77.8",
        "typescript": "^5.5.4",
        "vite": "^5.4.2",
        "vite-plugin-vuetify": "^2.0.4",
        "vitest": "^2.0.5",
        "vue-tsc": "^2.0.29"
