{
  "AllowedHosts": "*",
  "Environment": "__Environment__",
  "Serilog": {
    "Using": [ "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "__DefaultLoggingLevel__",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": ".\\logs\\agentcenter.api.log.txt",
          "rollingInterval": "Day"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName" ]
  },
  "DocumentServiceUrl": "__DocumentServiceUri__",
  "PolicyInfoServiceBaseUri": "__PolicyInfoServiceBaseUri__",
  "CommissionsServiceBaseUri": "__CommissionsServiceBaseUri__",
  "DebtServiceBaseUri": "__DebtServiceBaseUri__",
  "AgentServiceBaseUri": "__AgentServiceBaseUri__",
  "GroupInventoryServiceBaseUri": "__GroupInventoryServiceBaseUri__",
  "ProductionCreditServiceBaseUri": "__ProductionCreditServiceBaseUri__",
  "ListBillServiceBaseUri": "__ListBillServiceBaseUri__",
  "ApplicationTrackerServiceBaseUri": "__ApplicationTrackerServiceBaseUri__",
  "ClaimsServiceBaseUri": "__ClaimsServiceBaseUri__",
  "TaxFormsServiceBaseUri": "__TaxFormsServiceBaseUri__",
  "DocuSignServiceBaseUri": "__DocuSignServiceBaseUri__",
  "IllustrationProUrl": "__IllustrationProUrl__",
  "IllustrationProAgentAccountUrl": "__IllustrationProAgentAccountUrl__",
  "LifePortraitsUri": "__LifePortraitsUri__",
  "Authentication": {
    "PingOne": {
      "Authority": "__Authentication.PingOne.Authority__",
      "Audience": "__Authentication.PingOne.Audience__"
    },
    "AzureAd": {
      "Authority": "__Authentication.AzureAd.Authority__",
      "Audience": "__Authentication.AzureAd.Audience__"
    },
    "IdentityServer": {
      "BaseUri": "__Authentication.IdentityServer.BaseUri__",
      "ClientId": "__Authentication.IdentityServer.ClientId__",
      "ClientSecret": "__Authentication.IdentityServer.ClientSecret__"
    }
  },
  "MongoDb": {
    "ConnectionString": "__MongoDb.ConnectionString__",
    "EventsDatabaseName": "Events",
    "AgentCenterDatabaseName": "AgentCenter",
    "BenefitOptionsCollection": "BenefitOptionsMapping",
    "PendingSubaccountsCollection": "PendingSubaccounts",
    "ImpersonationLogCollection": "ImpersonationLog",
    "UserSearchCollection": "UserSearch",
    "ExcludedAgentIdsCollection": "ExcludedAgentIds",
    "UseTls": "true"
  },
  "Vertafore": {
    "Url": "__Vertafore.Url__",
    "Salt": "__Vertafore.Salt__",
    "Secret": "__Vertafore.Secret__"
  },
  "DocumentVault": {
    "BaseUri": "__DocumentVault.BaseUri__"
  },
  "DocumentQueue": {
    "HostName": "__DocumentQueue.HostName__",
    "Username": "__DocumentQueue.Username__",
    "Password": "__DocumentQueue.Password__"
  },
  "PdfToTiffBaseUrl": "__PdfToTiffBaseUrl__",
  "ConnectionStrings": {
    "GlobalData": "__ConnectionStrings.GlobalData__",
    "DataStore": "__ConnectionStrings.DataStore__",
    "AssureLink": "__ConnectionStrings.AssureLink__"

  },
  "TakeAction": {
    "MaximumFileLengthInBytes": "__TakeAction.MaximumFileLengthInBytes__",
    "MaximumFilesPerUpload": "__TakeAction.MaximumFilesPerUpload__"
  },
  "IPipeline": {
    "ConnectionString": "__IPipeline.ConnectionString__",
    "TargetString": "__IPipeline.TargetString__",
    "IPipelineDomain": "ipipeline.com",
    "ZanderCompanyId": "5802",
    "AssurityCompanyId": "3046",
    "AssurityGroupName": "03046-Assurity iGO Forms Users",
    "ZanderGroupName": "05802-UsersGroup",
    "ZanderTimeoutUrl": "https://info.assurity.com",
    "AssurityTimeoutUrl": "https://assurelink.assurity.com/default.aspx?iPipeErr=1",
    "AssuritySamlIssuer": "__IPipeline.AssuritySamlIssuer__",
    "Domain": "ipipeline.com",
    "ZanderAgentId": "__IPipeline.ZanderAgentId__",
    "AudienceString": "__IPipeline.AudienceString__"
  },
  "SmtpHost": "email.assurity.com",
