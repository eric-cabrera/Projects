  "ElasticSearch": {
    "BaseUri": "__ElasticSearchBaseUri__",
    "ApiKey": "__ElasticSearchApiKey__",
    "UserSearchIndex": "__ElasticSearchUserSearchIndex__"
  },
  "BaseAgentCenterUrl": "__BaseAgentCenterUrl__"
}