<!-- For more information see: https://newrelic.com/docs/dotnet/dotnet-agent-configuration -->
<configuration xmlns="urn:newrelic-config" agentEnabled="true">
	<distributedTracing enabled="true" />
	<applicationPools>
		<defaultBehavior instrument="true" />
	</applicationPools>
	<service licenseKey="" />
	<application>
		<name>Assurity.AgentCenter.Api.Local</name>
	</application>
	<log level="info" filename="newrelic.log" />
	<transactionTracer enabled="true" transactionThreshold="apdex_f" stackTraceThreshold="500" recordSql="obfuscated" explainEnabled="false" explainThreshold="500" />
	<crossApplicationTracer enabled="true" />
	<errorCollector enabled="true">
	</errorCollector>
	<browserMonitoring autoInstrument="true" />
	<labels>environment:Local;system:agentCenter</labels>
	<applicationLogging enabled="true">
		<forwarding enabled="true">
			<contextData enabled="true" include="" exclude="" />
		</forwarding>
	</applicationLogging>
</configuration>